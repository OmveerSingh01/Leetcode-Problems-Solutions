class Solution {
    public int maxProfit(int[] prices) {
    int minP = Integer.MAX_VALUE;
    int maxP =0;
    for(int i=0;i<prices.length;i++){
        if(minP < prices[i]){
           int p = prices[i] - minP;
             maxP = Math.max(maxP,p);
        }
        else{
            minP = prices[i];
        }
    }
    return maxP;
         }
    }
